        LIMIT #{offset}, #{pageSize}
    </select>

    <select id="selectByTypeAndCreateTimeAfter" resultType="com.habitual.demo.order.entity.RealOrderEntity">
        SELECT
            id, create_by, create_time, update_by, update_time, remark,
            business_id, user_id, tel, real_name, order_code, unit_price, total_price, num, type, status
        FROM real_order
        WHERE 1=1
          AND type = #{type}
          AND create_time >= #{createTimeAfter}
    </select>

    <select id="findAllReal" resultType="com.habitual.demo.order.entity.RealOrderEntity">
        SELECT
            id, create_by, create_time, update_by, update_time, remark,
            business_id, user_id, tel, real_name, order_code, unit_price, total_price, num, type, status
        FROM real_order
        WHERE 1=1
